@Article{oai:works.bepress.com:andrew_mccallum-1189,
  title =	"Mixtures of Hierarchical Topics with Pachinko
		 Allocation",
  author =	"David Mimno and Wei Li and Andrew McCallum",
  publisher =	"SelectedWorks",
  abstract =	"The four-level Pachinko Allocation model (PAM)
		 represents correlations among topics using a DAG
		 structure. It does not, however, represent a nested
		 hierarchy of topics, with some topical word
		 distributions representing the vocabulary that is
		 shared among several more specific topics. This paper
		 presents Hierarchical PAM---an enhancement that
		 explicitly represents a topic hierarchy. This model can
		 be seen as combining the advantages of hLDA's topical
		 hierarchy representation with PAM's ability to mix
		 multiple leaves of the topic hierarchy. Experimental
		 results show improvements in likelihood of held-out
		 documents, as well as mutual information between
		 automatically-discovered topics and human-generated
		 categories such as journals and newsgroups.",
  bibsource =	"OAI-PMH server at scholarworks.umass.edu",
  oai =  	"oai:works.bepress.com:andrew_mccallum-1189",
  source =	"Andrew McCallum",
  subject =	"Computer Sciences",
  URL =  	"http://works.bepress.com/andrew_mccallum/107;
		 http://works.bepress.com/cgi/viewcontent.cgi?article=1189\&context=andrew_mccallum",
}

